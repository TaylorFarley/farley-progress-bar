    "@sendgrid/mail": "^7.4.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.3.2",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "env-cmd": "^10.1.0",
    "express": "~4.16.1",
    "http-server": "^0.12.3",
    "is-empty": "^1.2.0",
    "json2csv": "^5.0.5",
    "jsonwebtoken": "^8.5.1",
    "material-ui-core": "^5.0.1",
    "mongoose": "^5.10.12",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "react-google-button": "^0.7.1",
    "react-scroll": "^1.8.1",
